# DocuMind Enhanced - Multi-Model API Configuration
# Copy this file to .env and add your actual API keys

# OpenAI API Key (required for GPT-4 comparison)
OPENAI_API_KEY=your_openai_api_key_here

# Mistral AI API Key (required for Mistral Large comparison)  
MISTRAL_API_KEY=your_mistral_api_key_here

# Anthropic/Claude API Key (required for Claude 3.5 Sonnet comparison)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
# Alternative key name (either works)
CLAUDE_API_KEY=your_claude_api_key_here

# Optional: Logging level
LOG_LEVEL=INFO

# Optional: Custom model configurations
OPENAI_MODEL=gpt-4
MISTRAL_MODEL=mistral-large-latest
CLAUDE_MODEL=claude-3-sonnet-20240229
